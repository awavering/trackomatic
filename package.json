    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "prompt": "^0.2.14",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.1"
  },
  "dependencies": {
    "domready": "^1.0.8"
