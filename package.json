  },
  "homepage": "https://github.com/vigetlabs/trackomatic",
  "devDependencies": {
    "assert": "^1.3.0",
    "async": "^1.4.2",
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "colors": "^1.1.2",
    "console.table": "^0.4.0",
    "esdoc": "^0.3.1",
    "esdoc-es7-plugin": "0.0.2",
    "io-spin": "^0.1.2",
